    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.1.13",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
