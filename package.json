    "axios": "^1.12.1",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "jose": "^6.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "15.5.2",
    "nuqs": "^2.6.0",
    "pg": "^8.16.3",
    "query-string": "^9.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
