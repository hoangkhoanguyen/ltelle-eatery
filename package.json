{
  "name": "ltelle-eatery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "find . -name '._*' -type f -delete",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "tsx watch server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint",
    "db:generate": "npm run clean && drizzle-kit generate",
    "db:migrate": "npm run clean && drizzle-kit migrate",
    "db:update": "npm run clean && drizzle-kit generate && drizzle-kit migrate"
  },
